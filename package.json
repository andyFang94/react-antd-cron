{
  "private": false,
  "name": "react-antd-cron",
  "version": "1.0.7",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npx standard-version && npm publish --access public",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andyFang94/react-antd-cron",
    "branch": "main"
  },
  "homepage": "https://andyfang94.github.io/react-antd-cron/",
  "author": "react-antd-cron",
  "keywords": [
    "antd",
    "ant",
    "design",
    "components",
    "react",
    "ui"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ant-design-colorful/ant-design-colorful/issues"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd": "4.17.2",
    "moment": "^2.29.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.1.0",
    "father-build": "^1.19.1",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "standard-version": "^9.3.2",
    "yorkie": "^2.0.0"
  }
}
